#ifndef TRIANGLE_H
#define TRIANGLE_H

class triangle {
private: 
    float a, b, c;
public: 
    triangle(a,b,c){
        this->a = a;
        this->b = b;
        this->c = c;
    }
    
   float perimeter (){
       return a+b+c;
   }
   float area (){
       float p = a+b+c;
       return sqrt( p*(p-a)*(p-b)*(p-c) )
   }
   
};

#endif // TRIANGLE_H
